{
  "name": "@generate_node/core",
  "version": "1.0.0",
  "description": "A flexible code generation tool with configurable templates and settings",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "bin": {
    "oa-generate": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "start": "node lib/index.js",
    "build": "npm run clean && cross-env NODE_ENV=production rollup -c",
    "build:dev": "npm run clean && rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["code-generator", "template", "oa", "automation"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.10",
    "html-beautifier": "^0.0.10",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.4",
    "rollup": "^4.0.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "@rollup/plugin-terser": "^0.4.0",
    "rimraf": "^5.0.0",
    "cross-env": "^7.0.3"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
